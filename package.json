{
    "name": "react-dropzone-material-ui",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://uraway.github.io/react-dropzone-material-ui",
    "dependencies": {
        "@material-ui/icons": "^4.2.1",
        "react-dropzone": "^10.1.5"
    },
    "scripts": {
        "build": "rollup -c",
        "build-watch": "rollup -c -w",
        "install-all": "yarn install && cd example && yarn install",
        "start-example": "cd example && yarn start",
        "deploy-example": "cd example && yarn run deploy",
        "dev": "concurrently --kill-others \"yarn run build-watch\" \"yarn run start-example\"",
        "test": "ts-node test/test.ts",
        "pretest": "yarn run build"
    },
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "files": [
        "dist"
    ],
    "types": "dist/index.d.ts",
    "devDependencies": {
        "@material-ui/core": "^4.2.0",
        "@types/jest": "24.0.15",
        "@types/node": "12.6.1",
        "@types/react": "16.8.23",
        "@types/react-dom": "16.8.4",
        "concurrently": "^4.1.1",
        "react-scripts": "3.0.1",
        "rollup": "^1.16.7",
        "rollup-plugin-typescript2": "^0.21.2",
        "typescript": "3.5.3"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.2.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    }
}
