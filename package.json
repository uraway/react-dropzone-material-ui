{
    "name": "react-dropzone-material-ui",
    "version": "2.0.0",
    "license": "MIT",
    "homepage": "https://uraway.github.io/react-dropzone-material-ui",
    "repository": {
        "type": "git",
        "url": "https://github.com/uraway/react-dropzone-material-ui"
    },
    "dependencies": {},
    "scripts": {
        "build": "rollup -c",
        "build-watch": "rollup -c -w",
        "install-all": "yarn install && cd example && yarn install",
        "start-example": "cd example && yarn start",
        "deploy-example": "cd example && yarn run deploy",
        "dev": "concurrently --kill-others \"yarn run build-watch\" \"yarn run start-example\"",
        "test": "echo \"No test found\"",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "pretest": "yarn run build",
        "prepublish": "yarn run build"
    },
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "files": [
        "dist"
    ],
    "types": "dist/index.d.ts",
    "devDependencies": {
        "@material-ui/core": "4.9.5",
        "@material-ui/icons": "4.9.1",
        "@types/jest": "24.0.15",
        "@types/node": "12.6.1",
        "@types/react": "16.8.23",
        "@types/react-dom": "16.9.17",
        "@typescript-eslint/eslint-plugin": "2.23.0",
        "@typescript-eslint/parser": "2.23.0",
        "concurrently": "4.1.2",
        "eslint": "6.8.0",
        "eslint-config-airbnb-typescript": "7.0.0",
        "eslint-config-prettier": "6.10.0",
        "eslint-config-standard-with-typescript": "14.0.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-node": "9.2.0",
        "eslint-plugin-prettier": "3.1.2",
        "eslint-plugin-promise": "4.2.1",
        "eslint-plugin-react": "7.19.0",
        "eslint-plugin-react-hooks": "1.7.0",
        "eslint-plugin-standard": "4.0.1",
        "prettier": "1.19.1",
        "react": "16.13.0",
        "react-dropzone": "10.2.1",
        "rollup": "1.32.1",
        "rollup-plugin-typescript2": "0.21.2",
        "typescript": "3.5.3"
    },
    "peerDependencies": {
        "@material-ui/core": ">=4.1",
        "@material-ui/icons": ">=4.0",
        "react": ">=16.8",
        "react-dropzone": ">=10.0"
    }
