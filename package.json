{
    "name": "react-dropzone-material-ui",
    "version": "1.0.0",
    "license": "MIT",
    "homepage": "https://uraway.github.io/react-dropzone-material-ui",
    "repository": {
        "type": "git",
        "url": "https://github.com/uraway/react-dropzone-material-ui"
    },
    "dependencies": {},
    "scripts": {
        "build": "rollup -c",
        "build-watch": "rollup -c -w",
        "install-all": "yarn install && cd example && yarn install",
        "start-example": "cd example && yarn start",
        "deploy-example": "cd example && yarn run deploy",
        "dev": "concurrently --kill-others \"yarn run build-watch\" \"yarn run start-example\"",
        "test": "echo \"No test found\"",
        "pretest": "yarn run build",
        "prepublish": "yarn run build"
    },
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "files": [
        "dist"
    ],
    "types": "dist/index.d.ts",
    "devDependencies": {
        "@material-ui/core": "^4.9.5",
        "@material-ui/icons": "^4.9.1",
        "@types/jest": "24.0.15",
        "@types/node": "12.6.1",
        "@types/react-dom": "16.8.4",
        "@types/react": "16.8.23",
        "babel-core": "^6.26.3",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "concurrently": "^4.1.1",
        "react-dropzone": "^10.2.1",
        "react-scripts": "3.0.1",
        "rollup": "^1.16.7",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.0.1",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-typescript2": "^0.21.2",
        "typescript": "3.5.3"
    },
    "peerDependencies": {
        "@material-ui/core": "^4.0.0",
        "@material-ui/icons": "^4.2.1",
        "react": "^16.8.0",
        "react-dom": "^16.8.0",
        "react-dropzone": "^10.1.5"
    }
}
